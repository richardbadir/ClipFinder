{
    "manifest_version": 2,
    "name": "ClipFinder",
    "version": "1.0",
    "description": "Ask questions to YouTube videos and get AI-generated answers.",
    "permissions": [
      "activeTab",
      "https://www.youtube.com/*",
      "storage",
      "tabs",
      "https://www.googleapis.com/",
      "downloads"
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icon2.png"
    }

  }
  